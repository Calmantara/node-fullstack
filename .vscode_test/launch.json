  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Graphql",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/ch4/app.js",
      "args": ["graphql"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Rest",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/ch5/app.js",
      "args": ["http"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Sequelize",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/ch4/app.js",
      "args": ["sequelize"]
    }
  ]
}
